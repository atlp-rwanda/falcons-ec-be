{
  "name": "falcons-ec-be",
  "version": "1.0.0",
  "description": "Falcons e-commerce team project",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node ./src/index",
    "build-babel": "babel -d ./build src/ -s",
    "clean": "rimraf build && mkdir build",
    "build": "npm run clean && npm run build-babel && npm run migrate && npm run seed",
    "dbMigrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "deMigrate:all:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "dbSeed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "test": "npm run deMigrate:all:test && npm run dbMigrate:test && npm run dbSeed:test && cross-env NODE_ENV=test nyc --exclude=src/controllers/cartController.js --exclude=src/utils/**/* --exclude=src/config/passport.js --exclude=src/services/product.service.js --exclude=src/jobs/checkExpiredPasswords.js --exclude=src/events/productEvents.js --exclude=src/helpers/random.js --exclude=src/helpers/stripePayment.js --exclude=src/routes/passport.js  --exclude=src/helpers/cart_retriever.js --exclude=src/controllers/recommend.js --reporter=lcov --reporter=html --reporter=text mocha --require @babel/register src/tests --timeout 60000 --exit",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/falcons-ec-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/falcons-ec-be/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/falcons-ec-be#readme",
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@sendgrid/mail": "^7.7.0",
    "assert": "^2.0.0",
    "axios": "^1.4.0",
    "base-64": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "install": "^0.13.0",
    "joi": "^17.8.4",
    "joi-password": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.2",
    "nodejs-nodemailer-outlook": "^1.2.4",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.21",
    "npm": "^9.6.2",
    "package-lock-only": "^0.0.4",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-stub": "^1.1.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^4.4.0",
    "sequelize": "^6.29.1",
    "sequelize-cli": "^6.6.0",
    "sinon": "^15.0.3",
    "socket.io": "^4.6.1",
    "stripe": "^11.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "typedarray-to-buffer": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  }
}
